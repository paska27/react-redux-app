{
  "name": "react-redux-app",
  "version": "0.0.0",
  "description": "React redux app boilerplate + workflow",
  "scripts": {
    "serve": "webpack-dev-server --progress --config webpack/config/dev.webpack.js",
    "build-dev": "webpack --progress --config webpack/config/dev.webpack.js",
    "flow": "flow status src",
    "flow-typed": "flow-typed install -r src --ignoreDeps dev",
    "lint:eslint": "eslint --config ./.eslintrc --ext .js --ext .jsx src",
    "lint:scss": "sass-lint -c .sasslintrc src/modules/**/*.scss -v -q"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "extend": "^3.0.1",
    "flow-bin": "^0.72.0",
    "flow-typed": "^2.4.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paska27/react-redux-app.git"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "Sergey Poskachey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paska27/react-redux-app/issues"
  },
  "homepage": "https://github.com/paska27/react-redux-app#readme"
}
